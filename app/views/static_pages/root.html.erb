<script id="bootstrap-current-user" type="text/javascript">
  <% if logged_in? %>
  	window.currentUser = <%= render("api/users/user.json.jbuilder",
  		user: current_user).html_safe %>
  <% end %>
  window.images = {
    header: "<%= image_path('header.jpg') %>",
    vcb_logo: "<%= image_path('vcb_logo.png') %>",
    menu_toggle: "<%= image_path('menu_toggle.png') %>"
   }
</script>
<main id="root"></main>
